<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.administrator.myApp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="我的第一个程序"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".EditTestActivity"></activity>
        <activity android:name=".AutoEditTestActivity"></activity>
        <activity android:name=".ButtonTestActivity"></activity>
        <activity android:name=".WebActivity"></activity>


    </application>
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>